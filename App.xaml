<Application x:Class="Password_Manager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Password_Manager"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--Order is Important-->
                <ResourceDictionary Source="Styles/Buttons.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Colors.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Fonts.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/MailContainer.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Rectangle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/ScrollBar.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/SearchBar.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/TextBox.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Texts.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Windows.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
